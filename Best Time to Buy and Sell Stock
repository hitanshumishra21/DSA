class Solution {
    public int maxProfit(int[] prices) {
        int buy_price = prices[0];
        int max_profit = 0;

        for(int i = 0;i < prices.length;i++){
            int curr_price = prices[i] - buy_price;
            if(prices[i]< buy_price){
                buy_price = prices[i];
            }else if(curr_price > max_profit){
                max_profit = curr_price;
            }
        }
        return max_profit;
    }
}
