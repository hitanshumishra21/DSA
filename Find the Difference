class Solution {
    public char findTheDifference(String s, String t) {
        char[] first = s.toCharArray();
        char[] last = t.toCharArray();

        Arrays.sort(first);
        Arrays.sort(last);
        
        for(int i=0;i<first.length;i++){
            if(first[i] != last[i])
                return last[i];
        }
        return last[last.length-1];
    }
}
